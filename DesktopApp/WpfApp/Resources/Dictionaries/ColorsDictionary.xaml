<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    >
    <!--Button Colors-->
    <SolidColorBrush x:Key="DefaultButtonColor" Color="#1F1F1F"/>
    <SolidColorBrush x:Key="DefaultButtonColorMO" Color="#2E2E2E"/>
    <SolidColorBrush x:Key="DefaultButtonColorMCW" Color="#151515"/>

    <SolidColorBrush x:Key="PDefaultButtonColor" Color="#121212"/>
    <SolidColorBrush x:Key="PDefaultButtonColorMO" Color="#212121"/>

    <SolidColorBrush x:Key="CloseButtonColorMO" Color="#E62E4C"/>
    <SolidColorBrush x:Key="CloseButtonColorMCW" Color="#A62036"/>

    <SolidColorBrush x:Key="BlueButtonColor" Color="#0074E5"/>
    <SolidColorBrush x:Key="BlueButtonColorMO" Color="#1381EC"/>
    <SolidColorBrush x:Key="BlueButtonColorMCW" Color="#0064C5"/>

    <SolidColorBrush x:Key="TextBoxColor" Color="#353535"/>


    <!--Common Colors-->
    <SolidColorBrush x:Key="NotEnabledColor" Color="#DDDDDD"/>
    <SolidColorBrush x:Key="MainBackground" Color="#121212"/>
    <SolidColorBrush x:Key="ButtonDarkGray" Color="#1F1F1F"/>
    <SolidColorBrush x:Key="ButtonRed" Color="#A62036"/>
    <SolidColorBrush x:Key="ButtonBlue" Color="#0074E5"/>
    <SolidColorBrush x:Key="ButtonGreen" Color="#00B048"/>
</ResourceDictionary>