<Application x:Class="WpfApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             DispatcherUnhandledException="Application_DispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Dictionaries/ButtonDictionary.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/ImagesDictionary.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/ScrollBarDictionary.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/TextBoxDictionary.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/ComboBoxDictionary.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/ContextMenuDictionary.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/ColorsDictionary.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/TableControls/Scale.Medium.xaml" />
                <ResourceDictionary Source="Resources/Dictionaries/ToolTipDictionary.xaml"/>
                <ResourceDictionary Source="Resources/Localizations/lang.en.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
